<div>
  <ul class="breadcrumb">
    <li><%= link_to 'Home',dashboard_home_index_path %></li>
    <li><%= link_to 'Examination',exams_path%></li>
    <li><%= link_to 'Report Center', report_center_exam_reports_path %></li>
    <li><%= link_to 'Archived student report', archived_student_report_exam_reports_path %> </li>
    <li>Generated report</li>
  </ul>
</div>
<div id="flash"><%= render 'layouts/messages'%></div>
<br/>
<div class="row">
  <div class="col-md-12">
    <div class="pull-right">
      <%= link_to 'Consolidated Report',consolidated_archived_report_exam_report_path(@batch.id), class: "btn btn-primary-link btn-sm" %>
      <%= link_to 'Pdf Report',archived_students_exam_report_exam_reports_path(id:@batch.id,format:"pdf"), class: "btn btn-primary-link btn-sm" %>
    </div>
  </div>
</div>
<hr>
<div class="col-md-2"></div>
  <h3><%= @batch.name %> - <%= @batch.course.course_name %></h3>
<br>
<div class="row">
  <div class="box col-md-3">
   <div>
    <%= render 'archived_student_list' %>
    </div>
  </div>
  <div class="box col-md-9">
  <div id="archived_student_report">
    <%= render 'archived_student_report' %>
  </div>
  </div>
</div>

 











